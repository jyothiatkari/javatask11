package package1;

public class Marks {
    private int marks;
    public void setMarks(int newmarks) {
    	if(marks>=0) {
    	marks=newmarks;
    	}
    	else {
    		System.out.println("invalid marks");
    	}
    }
    public int getMarks() {
    	return marks;
    }
}


//MarksELC class

package package1;

public class MarksMain {

	public static void main(String[] args) {
		 Marks m=new Marks();
		 m.setMarks(20);
		 System.out.println("The marks of the student is"+m.getMarks());
	}

}
