package package1;
import java.util.*;
public class WildcardP {
    
	public static void main(String[] args) {
	
		Scanner sc=new Scanner(System.in);
	       System.out.println("Enter two numbers");
	       int a=sc.nextInt();
	       int b=sc.nextInt();
	       System.out.println("Addition of two numbers is"+(a+b));
	       sc.close();

	}

}
